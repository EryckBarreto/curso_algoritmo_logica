Algoritmo "dardo"


Var

   a, b, c : real

Inicio

   escreval("Digite as três distâncias: ")
   leia(a)
   leia(b)
   leia(c)
   
   se (a > b) e (a > c) entao
   escreva("Maior distância: ", a)
   senao
      se b > c entao
      escreva("Maior distância: ", b)
      senao
      escreva("Maior distância: ", c)
      fimse
   fimse


Fimalgoritmo